{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb-base","prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "prefer-destructuring": ["error", {"object": true, "array": false}]
    }
}
